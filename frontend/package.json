{
  "name": "spa-frontend",
  "version": "2025.10.1",
  "private": true,
  "description": "Angular SPA Frontend",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl --ssl-key ../src/config/resources/tls/key.pem --ssl-cert ../src/config/resources/tls/certificate.crt --proxy-config proxy.conf.json",
    "dev": "ng serve --ssl --ssl-key ../src/config/resources/tls/key.pem --ssl-cert ../src/config/resources/tls/certificate.crt --proxy-config proxy.conf.json --open",
    "dev:http": "ng serve --proxy-config proxy.conf.json --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@angular/animations": "^21.0.3",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@ng-bootstrap/ng-bootstrap": "19.0.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@playwright/test": "1.57.0",
    "@types/node": "^24.10.1",
    "typescript": "5.9.3"
  }
}
